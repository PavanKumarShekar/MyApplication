<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kumar.pavan.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/safe_place"
        android:label="@string/app_name"
        android:roundIcon="@drawable/safe_place"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >



        <activity android:name=".moo.SpashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".moo.WelcomeActivity"
            android:screenOrientation="portrait"></activity>


        <activity android:name=".moo.HomeScreenActivity"
            android:screenOrientation="portrait"></activity>

        <activity android:name=".moo.ReportActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.ElevatorMalfunctionActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.FireHazardActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.RobberyActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.SexualAssaultActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.OtherActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.ReportLoggerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.LocationActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.SafetyActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".moo.SosReportActivity"
            android:screenOrientation="portrait"></activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>


</manifest>
